@model ImprovedWorkCenter.Models.Socio

@{
    ViewData["Title"] = "CalificarDeudor";
}

<h1>Calificar Deudor</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CalificarDeudor">
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="EsDeudor" /> @Html.DisplayNameFor(model => model.EsDeudor)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
